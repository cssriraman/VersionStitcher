<?xml version="1.0"?>
<package>
  <metadata>
    <id>VersionStitcher</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>http://opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>https://github.com/picrap/VersionStitcher</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/picrap/VersionStitcher/master/Icon/VersionStitcher.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Injects version information at build time (no source code change)</summary>
    <description>Injects version from source control information (git) based on explicit text patterns in assembly</description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags></tags>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src="VersionStitcher.nuspec" target="build"/>
    <file src="bin\$ConfigurationName$\VersionStitcher.exe" target="."/>
  </files>
</package>
